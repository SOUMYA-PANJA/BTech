   "source": [
    "a = int(input(\"Enter a number: \"))\n",
    "b = a\n",
    "c = 0\n",
    "while(b > 0):\n",
    "    c = c*10 + b%10\n",
    "    b = b//10\n",
    "if(a == c):\n",
    "    print(\"Palindrome\")\n",
    "else:\n",
    "    print(\"Not a palindrome.\")"
   ]
  }
