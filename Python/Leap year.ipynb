   ],
   "source": [
    "a = int(input(\"Enter year: \"))\n",
    "if(a%400 == 0):\n",
    "    print(\"Leap year.\")\n",
    "elif(a%100 == 0):\n",
    "    print(\"Not a leap year.\")\n",
    "elif(a%4 == 0):\n",
    "    print(\"Leap year.\")\n",
    "else:\n",
    "    print(\"Not a leap year.\")"
   ]
