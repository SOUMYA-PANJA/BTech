   "source": [
    "a = int(input(\"Enter a number: \"))\r\n",
    "b = a\r\n",
    "c = 0\r\n",
    "while(b > 0):\r\n",
    "    c += (b%10)**3\r\n",
    "    b = b//10\r\n",
    "if(a == c):\r\n",
    "    print(\"Armstrong number.\")\r\n",
    "else:\r\n",
    "    print(\"Not a Armstrong number.\")"
   ],
